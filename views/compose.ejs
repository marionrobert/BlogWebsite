<%- include("partials/header") -%>

  <h1>Compose</h1>
  <form action="/compose" method="post">
    <div class="form-group">
      <label>Title</label>
      <input type="text" name="postTitle" class="form-control">
      <label>Post</label>
      <textarea name="postContent" rows="8" cols="80" class="form-control"></textarea>
    </div>
    <button type="submit" name="button" class="btn btn-primary">Publish</button>
  </form>

<%- include("partials/footer"); -%>
